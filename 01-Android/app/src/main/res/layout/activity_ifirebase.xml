<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IFirebase">

    <ListView
        android:id="@+id/lv_firestore"
        android:layout_width="311dp"
        android:layout_height="405dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.895" />

    <Button
        android:id="@+id/btn_fs_epaginar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:text="E. Paginar"
        app:layout_constraintBottom_toTopOf="@+id/lv_firestore"
        app:layout_constraintEnd_toEndOf="@+id/lv_firestore"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/lv_firestore" />

    <Button
        android:id="@+id/btn_fs_ind_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:text="In. Comp"
        app:layout_constraintBottom_toTopOf="@+id/btn_fs_epaginar"
        app:layout_constraintEnd_toEndOf="@+id/btn_fs_epaginar"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_fs_epaginar" />

    <Button
        android:id="@+id/btn_fs_crear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Crear"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="123dp" />

    <Button
        android:id="@+id/btn_fs_eliminar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="Eliminar"
        app:layout_constraintBottom_toBottomOf="@+id/btn_fs_crear"
        app:layout_constraintStart_toEndOf="@+id/btn_fs_crear"
        app:layout_constraintTop_toTopOf="@+id/btn_fs_crear"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_fs_datos_prueba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:text="C. Datos"
        app:layout_constraintBottom_toTopOf="@+id/btn_fs_ind_comp"
        app:layout_constraintEnd_toEndOf="@+id/btn_fs_ind_comp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_fs_ind_comp" />

    <Button
        android:id="@+id/btn_fs_order_by"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:text="Order By"
        app:layout_constraintBottom_toTopOf="@+id/btn_fs_crear"
        app:layout_constraintEnd_toEndOf="@+id/btn_fs_crear"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_fs_crear" />

    <Button
        android:id="@+id/btn_fs_odoc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:text="O. Doc"
        app:layout_constraintBottom_toTopOf="@+id/btn_fs_eliminar"
        app:layout_constraintEnd_toEndOf="@+id/btn_fs_eliminar"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="@+id/btn_fs_eliminar" />

    <Button
        android:id="@+id/btn_fs_paginar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:text="Paginar"
        app:layout_constraintBottom_toTopOf="@+id/lv_firestore"
        app:layout_constraintEnd_toEndOf="@+id/lv_firestore"
        app:layout_constraintHorizontal_bias="0.672"
        app:layout_constraintStart_toStartOf="@+id/lv_firestore" />
</androidx.constraintlayout.widget.ConstraintLayout>